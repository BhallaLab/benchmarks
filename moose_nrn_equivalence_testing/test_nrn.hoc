create soma 
soma {
    nseg = 1
    diam = 18.8
    L = 18.8
    /*Ra = 123.0 * 1e-4*/
    Rm = 2.8 * 1e4
    cm = 1
    insert pas g_pas=1/Rm Ra=50 e_pas=-60.0  cm=1
    insert na3 gbar_na3 = 1e-4*1200 e_na3=50 
    insert nax gbar_nax = 1e-4*125 e_nax=50 e_na=50.0
    insert kap gbar_kap = 1e-4*36 e_kap=-90.0 e_k=-90.0
    insert kdr gbar_kdr = 1e-4*100 e_kdr=-90.0 e_k=-90.0
    insert hd gbar_hd = 1e-4*0.05 e_hd=-30.0 
    insert kad gbar_kad = 1e-4*60 e_kad=-90.0 
}

objref somaRec, tRec,  somaF

somaF = new File()

somaRec = new Vector()
tRec = new Vector()
somaRec.record(&soma.v(0.5))
tRec.record(&t)

objectvar stim
soma stim = new IClamp(0.5)
stim.amp = 1
stim.dur = 100
stim.del = 0

load_file("stdrun.hoc")
cvode.active(1)
finitialize()
tstop = 100
run()

// save the data
somaF.wopen("nrn_test.dat")
somaF.printf("t,soma\n")
for i=0,tRec.size()-1 {
    somaF.printf("%d %d\n", tRec.x(i), somaRec.x(i))
}

